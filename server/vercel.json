{
  "version" : 2,
  "builds" : [
    {
      "src" : "src/index.js",
      "use" : "@vecel/node"
    }
  ],
  "routes" : [
    {
      "src" : "/(.*)",
      "dest" : "src/index.js",
      "methods" : ["GET","POST","PUT","DELETE","PAATCH","OPTIONS"],
      "headers" : {"Access-Control-Allow-Origin" : "*"}
    }
  ]
}